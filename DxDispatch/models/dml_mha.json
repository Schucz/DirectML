{
    "$schema": "./_schema.json",
    
    "resources": 
    {
        "stackedQueryKeyValue": 
        {
            "initialValuesDataType": "FLOAT16",
            "initialValues": { "valueCount": 384, "valueStart": 0, "valueDelta": 0.01 }
        },
        "output": 
        {
            "initialValuesDataType": "FLOAT16",
            "initialValues": { "valueCount": 128, "value": 0 }
        }
    },

    "dispatchables": 
    {
        "mha": 
        {
            "type": "DML_OPERATOR_MULTIHEAD_ATTENTION",
            "desc": 
            {
                "StackedQueryKeyValueTensor": { "DataType": "FLOAT16", "Sizes": [2,2,1,3,32] },
                "OutputTensor": { "DataType": "FLOAT16", "Sizes": [2,2,32] },
                "Scale": 0.001,
                "MaskFilterValue": -10000000,
                "HeadCount": 1,
                "MaskType": "DML_MULTIHEAD_ATTENTION_MASK_TYPE_NONE"
            },
            "executionFlags":[
                "DML_EXECUTION_FLAG_ALLOW_HALF_PRECISION_COMPUTATION"
            ]
        }
    },

    "commands": 
    [
        {
            "type": "dispatch",
            "dispatchable": "mha",
            "bindings": 
            {
                "StackedQueryKeyValueTensor": "stackedQueryKeyValue",
                "OutputTensor": "output"
            }
        },
        { "type": "print", "resource": "output" },
        { "type": "printInitTemporary" },
        { "type": "printExecTemporary" }
    ]
}
            {
                "InputCount": 1,
                "OutputCount": 1,
                "Nodes": 
                [
                    { "type": "DML_GRAPH_NODE_TYPE_OPERATOR", "Operator": "node_0" },
                    { "type": "DML_GRAPH_NODE_TYPE_OPERATOR", "Operator": "node_1" },
                    { "type": "DML_GRAPH_NODE_TYPE_OPERATOR", "Operator": "node_2" }
                ],
                "InputEdges":
                [
                    { "GraphInputIndex": 0, "ToNodeIndex": 0, "ToNodeInputIndex": 0 },
                    { "GraphInputIndex": 0, "ToNodeIndex": 1, "ToNodeInputIndex": 0 }
                ],
                "OutputEdges":
                [
                    { "FromNodeIndex": 2, "FromNodeOutputIndex": 0, "GraphOutputIndex": 0 }
                ],
                "IntermediateEdges":
                [
                    { "FromNodeIndex": 0, "FromNodeOutputIndex": 0, "ToNodeIndex": 1, "ToNodeInputIndex": 0 },
                    { "FromNodeIndex": 1, "FromNodeOutputIndex": 0, "ToNodeIndex": 2, "ToNodeInputIndex": 1 }
                ]
            }
        }
    },

    "commands": 
    [
        { "type": "print", "resource": "y" }
    ]
